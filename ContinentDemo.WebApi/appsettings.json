{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "AppSettings": {

    "NetWorkRequestHost": "https://places-dev.continent.ru/",
    "NetWorkRequestUri": "airports",
    "NetWorkRetryCount": "3",

    "LocalCacheInitialCapacity": "17600",
    "LocalCacheExpiresAfterHours": "24",

    "UseDistributedCache": "True",
    "CachingOperationTimeOut_ms": "3000",

    "RedisCacheConfigurationString": "localhost",
    "RedisCacheInstanceName": "location_cache_",
    "RedisAbsoluteExpirationHours": "48",
    "RedisSlidingExpirationHours": "24",

    "ExtendedLogEnabled": "False",
    "UseTimer": "True"
  }
}
